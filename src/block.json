{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "easy-post-slider/slider",
	"version": "0.1.0",
	"title": "Easy Post Slider",
	"category": "widgets",
	"icon": "cover-image",
	"description": "A custom dynamic block to query and display your posts in a slider component.",
	"attributes": {
		"content": {
			"type": "object",
			"default": {
				"alignment": "center-center",
				"showTitle": true,
				"showExcerpt": true,
				"showLink": true,
				"minHeight": null,
				"background": true,
				"bgColor": "#e7ecef",
				"transparentBg": false,
				"fontColor": "black"
			}
		},
		"buttons": {
			"type": "object",
			"default": {
				"fontColor": "black",
				"bgColor": "#f6fff8"
			}
		},
		"scrollbar": {
			"type": "object",
			"default": {
				"showScrollbar": true,
				"color": "#f6fff8"
			}
		},
		"coverImage": {
			"type": "object",
			"default": {
				"opacity": 0,
				"overlayColor": "#f6fff8",
				"fontColor": "black"
			}
		},
		"query": {
			"type": "object",
			"default": {
				"posts_per_page": 1,
				"cat": 1,
				"tag_slug__in": []
			}
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "easy-post-slider",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"script": "file:./assets/js/index.ts",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
